{
  "name": "prose-mirror-react-wrapper",
  "version": "1.0.14",
  "description": "Basic React wrapper for ProseMirror",
  "main": "build/index.js",
  "types": "build/types/index.d.ts",
  "scripts": {
    "start": "webpack serve --config webpack.config.dev.js",
    "build": "webpack && tsc",
    "build:demo": "webpack --config webpack.config.demo.js",
    "test": "jest --silent",
    "lint": "eslint ./src/",
    "coverage": "npm run test -- --coverage",
    "prepare": "npm run build",
    "trypublish": "npm publish || true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/littlemyx/ProseMirrorReactWrapper"
  },
  "author": "Andrei Mukhin <littlemyx@gmail.com> (https://631am.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/littlemyx/ProseMirrorReactWrapper/issues"
  },
  "homepage": "https://github.com/littlemyx/ProseMirrorReactWrapper",
  "keywords": [
    "library",
    "react",
    "es6"
  ],
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "@types/prosemirror-commands": "~1.0.4",
    "@types/prosemirror-history": "1.0.2",
    "@types/prosemirror-keymap": "~1.0.4",
    "@types/prosemirror-schema-basic": "1.0.2",
    "@types/prosemirror-schema-list": "~1.0.3",
    "@types/prosemirror-state": "~1.2.8",
    "@types/prosemirror-transform": "~1.1.5",
    "prosemirror-commands": "~1.2.1",
    "prosemirror-example-setup": "^1.2.1",
    "prosemirror-history": "1.1.3",
    "prosemirror-keymap": "~1.1.5",
    "prosemirror-menu": "^1.2.1",
    "prosemirror-schema-basic": "1.1.2",
    "prosemirror-schema-list": "~1.1.6",
    "prosemirror-state": "~1.3.4",
    "prosemirror-transform": "~1.3.3",
    "prosemirror-view": "^1.17.3",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-popper": "^2.3.0",
    "use-prosemirror": "1.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.18.6",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-transform-typescript": "^7.18.12",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@playwright/test": "^1.25.0",
    "@types/jest": "^28.1.6",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/webpack-env": "^1.17.0",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.5",
    "babel-preset-minify": "^0.5.2",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "eslint": "^7.32.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.1.3",
    "mini-css-extract-plugin": "^2.6.1",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.3",
    "typescript": "4.1.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "4.9.3"
  },
  "resolutions": {
    "prosemirror-view": "~1.17.3"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "e2e"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/scripts/testMock.js",
      "\\.(css|less)$": "<rootDir>/scripts/testMock.js"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
